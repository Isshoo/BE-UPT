{
  "name": "upt-pik-backend",
  "version": "1.0.0",
  "description": "Backend API for UPT-PIK Application",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "dev": "nodemon src/index.js",
    "start": "node src/index.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:seed": "node prisma/seed.js",
    "prisma:seedAdmin": "node prisma/seedAdmin.js",
    "prisma:seedDosen": "node prisma/seedDosen.js",
    "prisma:seedFakultasProdi": "node prisma/seedFakultasProdi.js",
    "prisma:clear": "node prisma/clearData.js",
    "prisma:studio": "prisma studio",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint --fix src/**/*.js",
    "format": "prettier --write \"src/**/*.js\"",
    "docker:build": "docker compose build",
    "docker:migrate": "docker compose run app npm run prisma:migrate",
    "docker:seed": "docker compose run app npm run prisma:seed",
    "docker:up": "docker compose up -d",
    "docker:studio": "docker compose exec app npm run prisma:studio",
    "docker:down": "docker compose down"
  },
  "keywords": [
    "express",
    "api",
    "marketplace",
    "umkm"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^6.17.0",
    "bcryptjs": "^3.0.2",
    "cloudinary": "^1.41.3",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "exceljs": "^4.4.0",
    "express": "^5.1.0",
    "express-rate-limit": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "multer-storage-cloudinary": "^4.0.0",
    "pdfkit": "^0.17.2",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "prisma": "^6.17.0"
  }
}
